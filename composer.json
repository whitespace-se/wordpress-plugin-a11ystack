{
  "name": "whitespace-se/wordpress-plugin-a11ystack",
  "description": "Wordpress plugin that adds a11ystack-specific enhancements and features to Wordpress",
  "type": "wordpress-plugin",
  "license": "AGPL-3.0-or-later",
  "authors": [
    {
      "name": "Whitespace Dev",
      "email": "code@whitespace.se"
    }
  ],
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "kylekatarnls/update-helper": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Whitespace\\A11ystack\\": "src/"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "vcs",
      "url": "git@github.com:whitespace-se/Modularity.git"
    }
  ],
  "require": {
    "composer/installers": "^1.0 || ^2.0"
  },
  "require-dev": {
    "wpackagist-plugin/advanced-custom-fields": "^5.12",
    "helsingborg-stad/modularity": "dev-whitespace as 2.12.2"
  },
  "extra": {
    "installer-name": "whitespace-a11ystack"
  }
}
